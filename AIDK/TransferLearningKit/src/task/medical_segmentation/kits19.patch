+def prepare_submission(folder_in, folder_out):
+    nii = subfiles(folder_in, suffix='.gz', join=False)
+    maybe_mkdir_p(folder_out)
+    for n in nii:
+        i = n.split('-')[-1][:-10]
+        shutil.copy(join(folder_in, n), join(folder_out, i + '.nii.gz'))
