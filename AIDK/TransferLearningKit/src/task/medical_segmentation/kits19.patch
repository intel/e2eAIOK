+def get_mnms_data(data_root):
+    files_raw = []
+    files_gt = []
+    for r, dirs, files in os.walk(data_root):
+        for f in files:
+            if f.endswith('nii.gz'):
+                file_path = os.path.join(r, f)
+                if '_gt' in f:
+                    files_gt.append(file_path)
+                else:
+                    files_raw.append(file_path)
+    return files_raw, files_gt
+
