+    fullres_stage['conv_kernel_sizes'] = [[1, 3, 3],
+                                        [1, 3, 3],
+                                        [3, 3, 3],
+                                        [3, 3, 3],
+                                        [3, 3, 3],
+                                        [3, 3, 3],
+                                        [3, 3, 3]]
+
