+        :param out_planes:
+        :param props:
+        :param override_stride:
+        """
+        super().__init__()
+
+        self.kernel_size = kernel_size
+        props['conv_op_kwargs']['stride'] = 1
+
+        self.stride = stride
+        self.props = props
+        self.out_planes = out_planes
+        self.in_planes = in_planes
