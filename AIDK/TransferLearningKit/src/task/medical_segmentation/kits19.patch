+def increase_batch_size(plans_file: str, save_as: str, bs_factor: int):
+    a = load_pickle(plans_file)
+    stages = list(a['plans_per_stage'].keys())
+    for s in stages:
+        a['plans_per_stage'][s]['batch_size'] *= bs_factor
