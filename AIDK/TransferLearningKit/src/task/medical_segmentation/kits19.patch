+    tasknames = [i.split('/')[-1] for i in tasks]
+    args = []
+    for t, tn in zip(tasks, tasknames):
+        args.append((join(output_base, tn + ".zip"), join(output_base, t)))
+    p.starmap(compress_folder, args)
+    p.close()
+    p.join()
